
<html>
<head>
    <title>@ViewData["Title"]</title>
</head>
<body>
    <h1>@ViewData["Header"]</h1>
    @{
        var student = ViewData["Student"] as bai2.Models.Student;
    }
    <div>
        StudentID: @student.StudentId
    </div>
    <div>
        Name: @student.Name
    </div>
    <div>
        Branch: @student.Branch
    </div>
    <div>
        Section: @student.Section
    </div>
</body>
</html>